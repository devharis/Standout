@{
    ViewBag.Title = "Index";
}
<div class="loginBox col-xs-5">
    <header>
        <ul class="col-xs-6 pull-right button-group clearfix">
            <li><a href="#" class="reg btn btn-xs btn-default"><span class="ui-button-text">Register</span></a></li>
            <li><a href="#" class="forgot btn btn-xs btn-default"><span class="ui-button-text">Forgot Password</span></a></li>
            
        </ul>
        <h4>SWTS Login</h4>
    </header>
    <section>
        <div class="alert alert-warning col-xs-11">
            Enter any letter and press Login
        </div>
        <form class="form-signin form-inline">
            <p class="form-group">
                <div class="form-group">
                    <input type="text" class="form-control" placeholder="Email address" required autofocus>
                </div>
                <div class="form-group">
                    <input type="password" class="form-control" placeholder="Password" required>
                </div>
                <button class="btn btn-sm btn-default" type="submit">Sign in</button>
            </p>
            <p class="clearfix">
                <div class="checkbox">
                    <label>
                        <input type="checkbox"> Keep me logged-in
                    </label>
                </div>
            </p>
        </form>
    </section>
</div>



